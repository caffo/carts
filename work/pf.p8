end

function move(char)
  if solid_area(char.x+char.dx, char.y+char.dy, char.w,char.h) then
    char.dx = 0
    char.dy = 0
    return true
  end

  char.x += char.dx
  char.y += char.dy

  char.dx = 0
  char.dy = 0
end

-- collision logic

function solid(x, y)
 val=mget(x, y)
 return fget(val, 1)
end

function solid_area(x,y,w,h)
 return
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end

function actor(x,y,s)
  a        = {}
  a.x      = x
  a.y      = y
  a.dx     = 0
  a.dy     = 0
  a.sprite = s
  a.w      = 0.4
  a.h      = 0.4
  add(actors,a)
  return a
end

function draw_actor(a)
  local sx = (a.x * 8) - 4
  local sy = (a.y * 8) - 4
  spr(a.sprite, sx, sy)
end

