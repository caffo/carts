pico-8 cartridge // http://www.pico-8.com
version 5
__lua__

function _init()
  char        = {}
  char.x      = 1.5
  char.y      = 1.5
  char.dx     = 0
  char.dy     = 0
  char.sprite = 0
  char.w      = 0.4
  char.h      = 0.4
end

function _draw()
  cls()
  map(0,0,0,0,16,16)
  print(char.x, 10,110,5)
  print(char.y, 10,120,5)
  -- print(solid(char.x,char.y), 10,30,5)

  local sx = (char.x * 8) - 4
  local sy = (char.y * 8) - 4
  spr(char.sprite, sx, sy)
end

function _update()
  control(char)
  move(char)
end


-- custom methods
function control(char)
  inertia = .5
  if (btn(0)) char.dx -= inertia
  if (btn(1)) char.dx += inertia
  if (btn(2)) char.dy -= inertia
  if (btn(3)) char.dy += inertia
end

function move(char)
  if solid_area(char.x+char.dx, char.y+char.dy, char.w,char.h) then
    char.dx = 0
    char.dy = 0
    return true
  end

  char.x += char.dx
  char.y += char.dy

  char.dx = 0
  char.dy = 0
end

-- collision logic

function solid(x, y)
 val=mget(x, y)
 return fget(val, 1)
end

function solid_area(x,y,w,h)
 return
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end


